{
    "AWSEBDockerrunVersion": 2,
    "containerDefinitions": [
        {
            "name": "frontend",
            "image": "cech92/multidocker-frontend",
            "hostname": "frontend",
            "essential": false,
            "memory": 128
        }, 
        {
            "name": "backend",
            "image": "cech92/multidocker-backend",
            "hostname": "backend",
            "essential": false,
            "memory": 128
        },
        {
            "name": "worker",
            "image":"cech92/multidocker-worker",
            "hostname": "worker",
            "essential": false,
            "memory": 128
        },
        {
            "name": "nginx",
            "image": "cech92/multidocker-nginx",
            "hostname": "nginx",
            "essential": true,
            "portMappings": [
                {
                    "hostPort": 80,
                    "cotnainerPort": 80
                }
            ],
            "links": ["frontend", "backend"],
            "memory": 128
        }
    ]
}